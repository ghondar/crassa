{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "crassa start",
    "build": "crassa build && npm run ttsc && crassa server-build",
    "dev": "crassa dev",
    "server:build": "crassa server-build",
    "g": "crassa g",
    "lint": "crassa lint",
    "count": "crassa count-lines",
    "start:dev": "crassa start-dev",
    "test": "crassa test --env=jsdom",
    "lint-staged": "lint-staged",
    "ttsc": "ttsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "crassa": {
    "displayName": "example",
    "platform": "ts",
    "aliases": {
      "utils": "./src/utils",
      "lib": "./src/lib",
      "reducers": "./src/reducers",
      "sagas": "./src/sagas",
      "components": "./src/components",
      "containers": "./src/containers",
      "api": "./src/api"
    }
  },
  "_moduleAliases": {
    "utils": "./src/utils",
    "lib": "./src/lib",
    "reducers": "./src/reducers",
    "sagas": "./src/sagas",
    "components": "./src/components",
    "containers": "./src/containers",
    "api": "./src/api"
  },
  "jest": {
    "moduleFileExtensions": [
      "prod.js",
      "dev.js"
    ],
    "moduleNameMapper": {
      "^utils$": "<rootDir>/src/utils",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^lib$": "<rootDir>/src/lib",
      "^lib/(.*)$": "<rootDir>/src/lib/$1",
      "^reducers$": "<rootDir>/src/reducers",
      "^reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^sagas$": "<rootDir>/src/sagas",
      "^sagas/(.*)$": "<rootDir>/src/sagas/$1",
      "^components$": "<rootDir>/src/components",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^containers$": "<rootDir>/src/containers",
      "^containers/(.*)$": "<rootDir>/src/containers/$1",
      "^api$": "<rootDir>/src/api",
      "^api/(.*)$": "<rootDir>/src/api/$1"
    },
    "resolver": null
  },
  "dependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@types/loadable__component": "5.13.1",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.6",
    "@types/redux-devtools": "3.0.47",
    "@types/redux-devtools-dock-monitor": "1.1.33",
    "@types/redux-devtools-log-monitor": "1.0.35",
    "@types/redux-logger": "3.0.8",
    "@types/webpack-env": "1.15.3",
    "@zerollup/ts-transform-paths": "1.7.18",
    "axios": "0.20.0",
    "connected-react-router": "6.8.0",
    "crassa": "file:../",
    "extensible-duck": "ghondar/extensible-duck",
    "history": "5.0.0",
    "react-hot-loader": "4.13.0",
    "react-router-dom": "5.2.0",
    "redux-logger": "3.0.6"
  },
  "devDependencies": {
    "@testing-library/react": "11.1.0",
    "redux-devtools": "3.7.0",
    "redux-devtools-dock-monitor": "1.2.0",
    "redux-devtools-log-monitor": "2.1.0",
    "ttypescript": "1.5.12",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,server}/**/*.js": [
      "eslint --ext .js --fix"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
