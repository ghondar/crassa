{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "start": "crassa start",
    "build": "crassa build && npm run ttsc && crassa server-build",
    "dev": "crassa dev",
    "server:build": "crassa server-build",
    "g": "crassa g",
    "lint": "crassa lint",
    "count": "crassa count-lines",
    "dev:start": "crassa dev-start",
    "start:dev": "crassa start-dev",
    "test": "crassa test --env=jsdom",
    "lint-staged": "lint-staged",
    "ttsc": "ttsc",
    "postinstall": "ttsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "crassa": {
    "displayName": "example",
    "platform": "ts",
    "aliases": {
      "utils": "./src/utils",
      "lib": "./src/lib",
      "reducers": "./src/reducers",
      "sagas": "./src/sagas",
      "components": "./src/components",
      "containers": "./src/containers",
      "api": "./src/api"
    }
  },
  "_moduleAliases": {
    "utils": "./src/utils",
    "lib": "./src/lib",
    "reducers": "./src/reducers",
    "sagas": "./src/sagas",
    "components": "./src/components",
    "containers": "./src/containers",
    "api": "./src/api"
  },
  "jest": {
    "moduleFileExtensions": [
      "prod.js",
      "dev.js"
    ],
    "moduleNameMapper": {
      "^utils$": "<rootDir>/src/utils",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^lib$": "<rootDir>/src/lib",
      "^lib/(.*)$": "<rootDir>/src/lib/$1",
      "^reducers$": "<rootDir>/src/reducers",
      "^reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^sagas$": "<rootDir>/src/sagas",
      "^sagas/(.*)$": "<rootDir>/src/sagas/$1",
      "^components$": "<rootDir>/src/components",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^containers$": "<rootDir>/src/containers",
      "^containers/(.*)$": "<rootDir>/src/containers/$1",
      "^api$": "<rootDir>/src/api",
      "^api/(.*)$": "<rootDir>/src/api/$1"
    },
    "resolver": null
  },
  "dependencies": {
    "@hot-loader/react-dom": "17.0.1",
    "axios": "0.21.1",
    "connected-react-router": "6.9.1",
    "crassa": "file:../",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "extensible-duck": "ghondar/extensible-duck",
    "history": "4.10.1",
    "husky": "5.1.3",
    "react-hot-loader": "4.13.0",
    "react-router-dom": "5.2.0",
    "redux-logger": "3.0.6"
  },
  "devDependencies": {
    "@testing-library/react": "11.2.5",
    "@types/loadable__component": "5.13.3",
    "@types/react-router-dom": "5.1.7",
    "@types/redux-devtools": "3.0.47",
    "@types/redux-devtools-dock-monitor": "1.1.33",
    "@types/redux-devtools-log-monitor": "1.0.35",
    "@types/redux-logger": "3.0.8",
    "@types/webpack-env": "1.16.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "redux-devtools": "3.7.0",
    "redux-devtools-dock-monitor": "1.2.0",
    "redux-devtools-log-monitor": "2.1.0",
    "ttypescript": "1.5.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,server}/**/*.ts?(x)": "eslint --ext .ts,tsx --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
